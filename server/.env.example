# MongoDB Atlas Connection (Get from https://www.mongodb.com/cloud/atlas)
MONGODB_URI=mongodb+srv://your-username:your-password@cluster0.xxxxx.mongodb.net/technobit26?retryWrites=true&w=majority

# JWT Secrets (use long random strings)
CLIENT_SECRET=your_secret_key_for_jwt_here_min_32_chars
ADMIN_SECRET=your_admin_secret_key_here_min_32_chars

# Admin Credentials
ADMIN_USER=your_admin_username
ADMIN_PASS=your_admin_password

# CORS Whitelist (comma-separated URLs)
REMOTE_CLIENT_APP=http://localhost:3000

# Port (for local development)
PORT=5000
